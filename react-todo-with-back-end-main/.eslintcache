( task.id === id){\n          task.title = value\n        }\n      })\n      this.setState({tasks: tasks})\n    })\n  }\n\n  // componentDidMount() {\n  //   fetch('http://localhost:8080/api/todoitems')\n  //   .then((response) => response.json())\n  //   .then((json) => this.setState({tasks: json}));\n  // }\n  componentDidMount() {\n    TodoService.getAll()\n    .then((json) => this.setState({tasks: json.data}));\n  }\n  render() {\n    console.log(this.state.tasks)\n    return (\n      <div>\n        <form>\n          <input type=\"text\" onInput={this.handleInput} value={this.state.newTask}/>\n          <button type=\"button\" onClick={this.handleClick}>Add</button>\n        </form>\n        <ul>\n          <TodoItems editItems = {this.editItems} tasks={this.state.tasks} foo=\"bar\" removeItem={this.removeItem}/>\n        </ul>\n      </div>\n    )\n  }\n\n  componentDidUpdate() {\n    console.log(this.state)\n  }\n}\n\nexport default TodoList;","C:\\Users\\maysa\\OneDrive\\Desktop\\react-todo-with-back-end-main\\src\\components\\TodoItems.js",[],"C:\\Users\\maysa\\OneDrive\\Desktop\\react-todo-with-back-end-main\\src\\services\\todo.service.js",[],"C:\\Users\\maysa\\OneDrive\\Desktop\\react-todo-with-back-end-main\\src\\http-common.js",[],{"ruleId":"35","replacedBy":"36"},{"ruleId":"37","replacedBy":"38"},{"ruleId":"39","severity":1,"message":"40","line":63,"column":22,"nodeType":"41","messageId":"42","endLine":63,"endColumn":24},"no-native-reassign",["43"],"no-negated-in-lhs",["44"],"array-callback-return","Array.prototype.map() expects a return value from arrow function.","ArrowFunctionExpression","expectedInside","no-global-assign","no-unsafe-negation"]